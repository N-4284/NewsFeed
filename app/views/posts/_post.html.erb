<div id="<%= dom_id post %>">
  <p>
    <strong>Author:</strong>
    <%= post.author_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Thumbnail:</strong>
    <%= post.thumbnail %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= post.category_id %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= post.location_id %>
  </p>

</div>
