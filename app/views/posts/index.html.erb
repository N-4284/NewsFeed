<p style="color: green"><%= notice %></p>

<h1>news feed page</h1>


<div id="posts">
  <% @posts.includes(:author,:category, :location).all.each do |post| %>
    <%= render post %>
    <p>
      <%= link_to "Show this post", post %>
    </p>
  <% end %>
</div>

<%= link_to "New post", new_post_path %>
