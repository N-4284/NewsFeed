<div class='container form-holder '>  
<div class="form-container">
<h1>New Category</h1>

<%= form_with(model: @category, url: moderator_create_category_path, local: true) do |form| %>
  <%= render 'categoryForm', form: form%>
  <div>
  <%= form.submit 'Create Category', class: "button" %>
  </div>
<% end %>

</div>
</div>